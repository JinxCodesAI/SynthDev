# General AI Provider Configuration
SYNTHDEV_API_KEY=your_base_model_api_key
SYNTHDEV_BASE_MODEL=default-model
SYNTHDEV_BASE_URL=https://api.example.com/v1

# Smart Model Configuration
SYNTHDEV_SMART_API_KEY=your_smart_model_api_key
SYNTHDEV_SMART_MODEL=smart-model
SYNTHDEV_SMART_BASE_URL=https://api.example.com/v1

# Fast Model Configuration
SYNTHDEV_FAST_API_KEY=your_fast_model_api_key
SYNTHDEV_FAST_MODEL=fast-model
SYNTHDEV_FAST_BASE_URL=https://api.example.com/v1

# Global Settings
SYNTHDEV_MAX_TOOL_CALLS=50
# Set to true to enable prompt enhancement, super unstable and experimental
SYNTHDEV_ENABLE_PROMPT_ENHANCEMENT=false

# Verbosity Level (0-5)
# 0 - Only information directly affecting the user is visible
# 1 - Short messages like "ðŸ”„ Enhancing prompt...", "ðŸ§  Synth-Dev is thinking..."
# 2 - Compressed arguments for tool execution (default)
# 3 - Uncompressed arguments but no tool results
# 4 - Both arguments and tool results
# 5 - Every HTTP request and response
SYNTHDEV_VERBOSITY_LEVEL=2